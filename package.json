{
  "name": "GitHubFollower",
  "version": "1.0.0",
  "description": "Application used to follow trending GitHub repositories",
  "main": "index.js",
  "scripts": {
    "start": "./node_modules/.bin/webpack-dev-server --config config/webpack.config.js",
    "build:server": "./node_modules/.bin/webpack --config config/webpack.prod.server.js",
    "build:client": "./node_modules/.bin/webpack --config config/webpack.prod.client.js",
    "build": "npm run build:server && npm run build:client",
    "server:dev": "development=true nodemon server/main.js --exec babel-node",
    "server:prod": "development=false babel-node server/main.js --exec babel-node",
    "server:debug": "development=true babel-node --inspect-brk server/main.js --exec babel-node",
    "server": "npm run server:dev"
  },
  "author": "Denimar de Moraes",
  "license": "ISC",
  "devDependencies": {
    "webpack": "^4.1.0",
    "webpack-cli": "^2.0.10",
    "webpack-dev-middleware": "^3.1.2",
    "webpack-dev-server": "^3.1.0",
    "webpack-hot-middleware": "^2.21.2",
    "webpack-node-externals": "^1.7.2"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "bootstrap": "^3.3.7",
    "copy-webpack-plugin": "^4.5.0",
    "css-loader": "^0.28.10",
    "debug": "^3.1.0",
    "deni-react-modal": "0.0.40",
    "deni-react-popup-menu": "0.0.5",
    "file-loader": "^1.1.11",
    "js-cookie": "^2.2.0",
    "moment": "^2.21.0",
    "mysql2": "^1.5.3",
    "node-sass": "^4.7.2",
    "nodemon": "^1.17.3",
    "react-bootstrap": "^0.32.1",
    "react-hot-loader": "^4.0.0",
    "react-icons": "^2.2.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "sass-loader": "^6.0.7",
    "sequelize": "^4.37.6",
    "style-loader": "^0.20.2",
    "apollo-boost": "^0.1.6",
    "apollo-client": "^2.3.1",
    "apollo-link": "^1.2.2",
    "apollo-link-http": "^1.5.4",
    "babel-runtime": "^6.26.0",
    "base-64": "^0.1.0",
    "body-parser": "^1.18.2",
    "child_process": "^1.0.2",
    "cors": "^2.8.4",
    "deni-react-dialog": "0.0.3",
    "deni-react-grid": "0.0.17",
    "deni-react-popover": "0.0.8",
    "deni-react-popover-selector": "0.0.35",
    "env.json": "^2.0.1",
    "express": "^4.16.3",
    "fs-extra": "^5.0.0",
    "github-api": "^3.0.0",
    "graphql": "^0.13.2",
    "graphql-tag": "^2.9.2",
    "jsonwebtoken": "^8.2.1",
    "load-json-file": "^4.0.0",
    "node-schedule": "^1.3.0",
    "react": "^16.4.0",
    "react-apollo": "^2.1.4",
    "react-dom": "^16.4.0",
    "react-google-maps": "^9.4.5",
    "recompose": "^0.27.0",
    "transform-runtime": "0.0.0"
  }
}
